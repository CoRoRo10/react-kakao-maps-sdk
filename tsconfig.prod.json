{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "commonjs",
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "jsx": "react",
    "sourceMap": true,
    "declaration": true,
    "esModuleInterop": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "removeComments": false,
    "types": ["kakao.maps.d.ts"]
  },
  "include": ["src/lib/Kakaomap/**/*.ts", "src/lib/Kakaomap/**/*.tsx"],
  "exclude": [
    "node_modules",
    "src/lib/Kakaomap/**/*.test.ts",
    "src/lib/Kakaomap/**/*.test.tsx"
  ]
}
